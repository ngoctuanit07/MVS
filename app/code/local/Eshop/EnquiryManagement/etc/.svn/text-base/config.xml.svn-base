<?xml version="1.0"?>
<config>
  <modules>
    <Eshop_EnquiryManagement>
      <version>0.1.0</version>
    </Eshop_EnquiryManagement>
  </modules>
  <frontend>
    <routers>
      <enquirymanagement>
        <use>standard</use>
          <args>
            <module>Eshop_EnquiryManagement</module>
            <frontName>enquirymanagement</frontName>
          </args>
      </enquirymanagement>
    </routers>
		<layout>
		  <updates>
			<enquirymanagement>
			  <file>enquirymanagement.xml</file>
			</enquirymanagement>
		  </updates>
		</layout>
  </frontend>
  <global>
    <helpers>
      <enquirymanagement>
        <class>Eshop_EnquiryManagement_Helper</class>
      </enquirymanagement>
    </helpers>
	<blocks>
	  <enquirymanagement>
		<class>Eshop_EnquiryManagement_Block</class>
	  </enquirymanagement>
	</blocks>
	<models>
	  <enquirymanagement>
		<class>Eshop_EnquiryManagement_Model</class>
		<resourceModel>enquirymanagement_mysql4</resourceModel>
	  </enquirymanagement>
	  <enquirymanagement_mysql4>
		<class>Eshop_EnquiryManagement_Model_Mysql4</class>
		<entities>		  
			  <enquirymanagement>
				<table>enquirymanagement</table>
			  </enquirymanagement>
        </entities>
	  </enquirymanagement_mysql4>
	</models>
	<resources>
	  <enquirymanagement_setup>
		<setup>
		  <module>Eshop_EnquiryManagement</module>
		</setup>
		<connection>
		  <use>core_setup</use>
		</connection>
	  </enquirymanagement_setup>
	  <enquirymanagement_write>
		<connection>
		  <use>core_write</use>
		</connection>
	  </enquirymanagement_write>
	  <enquirymanagement_read>
		<connection>
		  <use>core_read</use>
		</connection>
	  </enquirymanagement_read>
	</resources>
  </global>
  <admin>
	<routers>
	  <enquirymanagement>
	    <use>admin</use>
		<args>
		  <module>Eshop_EnquiryManagement</module>
		  <frontName>enquirymanagement</frontName>
		</args>
	  </enquirymanagement>
	</routers>
  </admin>
  <adminhtml>
	<menu>
	  <enquirymanagement module="enquirymanagement">
		<title>EnquiryManagement</title>
		<sort_order>100</sort_order>
		<children>
		  <enquirymanagement module="enquirymanagement">
		    <title>Manage Enquirymanagement</title>
			<sort_order>0</sort_order>
			<action>enquirymanagement/adminhtml_enquirymanagement</action>
		  </enquirymanagement>
		</children>
	  </enquirymanagement>
	</menu>
	<acl>
	  <resources>
		<all>
		  <title>Allow Everything</title>
		</all>
		<admin>
		  <children>
			<enquirymanagement translate="title" module="enquirymanagement">
			  <title>EnquiryManagement</title>
			  <sort_order>1000</sort_order>
			  <children>
		  <enquirymanagement translate="title">
			<title>Manage Enquirymanagement</title>
			<sort_order>0</sort_order>
		  </enquirymanagement>
			  </children>
			</enquirymanagement>
		  </children>
		</admin>
	  </resources>
	</acl>
	<layout>
	  <updates>
		<enquirymanagement>
		  <file>enquirymanagement.xml</file>
		</enquirymanagement>
	  </updates>
	</layout>
  </adminhtml>
</config> 